<div class="col-md-6">
    <div class="row">
        <div class="col-md-4">
            <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-warning btn-block">Edit</a>
        </div>
        <div class="col-md-4">
            <a asp-action="Index" class="btn btn-warning btn-block">Back to List</a>
        </div>
       
